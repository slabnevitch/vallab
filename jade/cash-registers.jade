extends _misc/main
block title
	| Vallab кассы
	- var page = "cashRegister";

block content
	include _sections/_cordless
	include _sections/_registers
	include _sections/_drives
	include _sections/_operators
	include _sections/_accompaniment
	.signup
		.signup__container
			form.form(action="#" data-signup name="cashRegisterForm")
				.form__top
					.form__title Вызвать консультанта

					input(autocomplete="off" type="text" required name="" class="form__input form__input--phone" placeholder="Телефон")

					input(autocomplete="off" type="text" required name="selectedCashRegister" class="form__input" placeholder="Касса")

					textarea.form__input.form__area(placeholder="Сообщение")

				.form__regard
					.form__title Спасибо за заявку. Мы с вами скоро свяжемся.

	include _sections/_quickly
	
	.fixed-block
		span(data-micromodal-open='gift-modal').fixed-block__gift.animated.tada.infinite
			img(src="img/gift.png" alt="alt")
		button.fixed-block__chat
			img(src="img/icons-svg/chat.svg" alt="alt")

	.modal.modal-gift#gift-modal.micromodal-slide(aria-hidden='true')
		.modal__overlay(tabindex='-1' data-micromodal-close)
			.modal-container(role='dialog' aria-modal='true' aria-labelledby='modal-1-title')
					button.modal__close(aria-label='Close modal' data-micromodal-close)
					main.modal__content
						form.form(action="#" data-signup name="cashRegisterForm")
							.form__top
								// .form__title Вызвать консультанта

								input(autocomplete="off" type="text" required name="" class="form__input form__input--phone" placeholder="Телефон")

								- const opts = ['ответы для Налоговой', 'разъяснения Налоговой спорных ситуаций', 'чек-лист проверки своих чеков', 'Тех.Задание для АйТи специалиста'];
								.form__select
									select(data-searchdisable="true" class="fstdropdown-select ")
										each opt, i in opts
											option(value=i)= opt
								button(type="submit" class="form__send form__send--wht") Получить подарок


